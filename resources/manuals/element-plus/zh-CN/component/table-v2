<span class="line"><span style="--shiki-light:#6F42C1;--shiki-dark:#B392F0">.hovering-col-2</span><span style="--shiki-light:#24292E;--shiki-dark:#E1E4E8"> [</span><span style="--shiki-light:#6F42C1;--shiki-dark:#B392F0">data-key</span><span style="--shiki-light:#D73A49;--shiki-dark:#F97583">=</span><span style="--shiki-light:#032F62;--shiki-dark:#9ECBFF">'hovering-col-2'</span><span style="--shiki-light:#24292E;--shiki-dark:#E1E4E8">],</span></span>
